{
    "configurations": [
        {
            "type": "node",
            "name": "vscode-jest-tests.v2.bitcoin-guesser",
            "request": "launch",
            "args": [
                "run",
                "test:unit",
                "--",
                "--runInBand",
                "--watchAll=false",
                "--testNamePattern",
                "${jest.testNamePattern}",
                "--runTestsByPath",
                "${jest.testFile}"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "disableOptimisticBPs": true,
            "runtimeExecutable": "npm"
        }
    ]
}